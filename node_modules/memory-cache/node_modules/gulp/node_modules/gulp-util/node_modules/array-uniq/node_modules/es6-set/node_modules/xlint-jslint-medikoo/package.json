{
  "name": "xlint-jslint-medikoo",
  "version": "0.1.4",
  "description": "JSLint mod configured as handler for XLint",
  "author": "Mariusz Nowak <medyk@medikoo.com> (http://www.medikoo.com/)",
  "keywords": [
    "jslint",
    "lint",
    "xlint",
    "jshint",
    "cli",
    "console",
    "medikoo"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/medikoo/xlint-jslint-medikoo.git"
  },
  "dependencies": {
    "xlint-jslint": "0.1"
  },
  "devDependencies": {
    "tad": "~0.2.2",
    "xlint": "~0.2.2"
  },
  "scripts": {
    "lint": "node node_modules/xlint/bin/xlint --linter=index.js --no-cache --no-stream",
    "lint-console": "node node_modules/xlint/bin/xlint --linter=index.js --watch",
    "test": "node ./node_modules/tad/bin/tad"
  },
  "license": "MIT"
}

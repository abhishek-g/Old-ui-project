{
  "name"            : "consolify",
  "version"         : "1.0.0",
  "keywords"        : ["browser", "console", "stdout", "ansi", "color", "browserify"],
  "description"     : "Turn your browser window into a console",
  "author"          : "Maximilian Antoni (http://maxantoni.de)",
  "homepage"        : "https://github.com/mantoni/consolify",
  "bin"             : "bin/cmd.js",
  "main"            : "./lib/consolify.js",
  "browser"         : "./lib/browser.js",
  "engines"         : {
    "node"          : ">=0.10"
  },
  "scripts"         : {
    "start"         : "mocha --watch",
    "lint"          : "jslint --color \"**/*.js\"",
    "test"          : "npm run lint && mocha"
  },
  "dependencies"    : {
    "through2"      : "^0.6",
    "listen"        : "^1.0",
    "browser-reload": "^1.0",
    "brout"         : "^1.0",
    "source-mapper" : "^1.0",
    "ansi_up"       : "^1.1"
  },
  "devDependencies" : {
    "jslint"        : "^0.6",
    "mocha"         : "^1.20",
    "browserify"    : "^5.9"
  },
  "repository"      : {
    "type"          : "git",
    "url"           : "https://github.com/mantoni/consolify.git"
  },
  "files"           : ["bin", "lib", "README.md", "CHANGES.md", "LICENSE"],
  "license"         : "MIT"
}
